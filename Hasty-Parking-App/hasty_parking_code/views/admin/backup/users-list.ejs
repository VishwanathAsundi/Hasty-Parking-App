  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper"> 
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1> Users List </h1>
      <ol class="breadcrumb">
        <li><i class="fa fa-dashboard"></i> Home</li>
        <li class="active">Users List</li>
      </ol>
    </section>
    
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <!-- /.box-header -->
            <div class="box-body">
              <table id="user_list" class="table table-bordered table-striped">
                <thead>
                  <tr>
                    <th>S.No </th>
                    <th>Customer Name</th>
                    <th>Email Address</th>
                    <th>Contact Number</th>
                    <th>Active From</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <tr ng-repeat="data in datas">
                    <td>1</td>
                    <td>data.firstname data.lastname</td>
                    <td>data.email</td>
                    <td>data.phone_number</td>
                    <td>data.created_at</td>
                    <td><label class="switch margin-bottom-none">
                      <input type="checkbox" name="disable">
                      <div class="slider round"></div>
                      </label>
                      <a href="users-details.ejs" class="u_view"><i class="fa fa-eye pull-right fa-2x" aria-hidden="true"></i></a></td>
                  </tr>
                </tbody>
              </table>
            </div>
            <!-- /.box-body --> 
          </div>
          <!-- /.box --> 
        </div>
        <!-- /.col --> 
      </div>
      <!-- /.row --> 
    </section>
    <!-- /.content --> 

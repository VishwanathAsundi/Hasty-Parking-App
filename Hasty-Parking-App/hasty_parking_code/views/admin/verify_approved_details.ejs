<!DOCTYPE html>
<html ng-app="app" ng-controller="verifyAppr_Ctrl" ng-init="getSpaceUserDetails();">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Hasty Parking | Data Tables</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="admin/css/bootstrap.min.css">
  <link rel="stylesheet" href="admin/css/styles.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -- >
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="admin/plugins/datatables/dataTables.bootstrap.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="admin/css/admin.min.css">
  <!-- Admin Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="admin/css/all-skins.min.css">
  <script type="text/javascript" src="admin/js/angularjs/default/angular.min.js"></script>
  <script type="text/javascript" src="admin/js/angularjs/user/app.js"></script>
 <script type="text/javascript" src="admin/js/angularjs/user/designer/verify_approved_details.js"></script>
  
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-green sidebar-mini">
<div class="wrapper">
  <% include ./partials/header %>
  <!-- Left side column. contains the logo and sidebar -->
  <% include ./partials/side-bar %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>Verification Onprogress List</h1>
      <ol class="breadcrumb">
        <li><i class="fa fa-dashboard"></i> Home</li>
        <li><a href="verify_onprogress.ejs">Verification Approved List</a></li>
        <li class="active">Verification Approved Details</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <div class="row">
        <div class="col-md-6">
          <!-- Profile Image -->
          <div class="box box-success">
            <div class="box-body Verification_details">
              <ul class="list-group list-group-unbordered">
                <li class="list-group-item">
                  <b>Owner Name</b> <span class="pull-right">{{datas.userdetails.customername}}</span>
                </li>
                <li class="list-group-item">
                  <b>Email</b> <a class="pull-right" href="name@email.com">{{datas.userdetails.email}}</a>
                </li>
                <li class="list-group-item">
                  <b>Space Heading</b> <span class="pull-right">Title of the space</span>
                </li>
                <li class="list-group-item">
                  <b>Location of the Space</b> <span class="pull-right">{{datas.spacedetails.location}}</span>
                </li>
                <li class="list-group-item">
                  <b>Postal code</b> <span class="pull-right">xxx xxx</span>
                </li>
                <li class="list-group-item">
                  <b>Type of space</b> <span class="pull-right">{{datas.spacedetails.space_type}}</span>
                </li>
                <li class="list-group-item">
                  <b>No. of space</b> <span class="pull-right">{{datas.spacecount}}</span>
                </li>
                <li class="list-group-item">
                  <b>Amnities</b> <span class="pull-right">{{datas.spacedetails.amenties}}</span>
                </li>
                <li class="list-group-item">
                  <b>Is this space Delinated?</b> <span class="pull-right">{{datas.spacedetails.space_delinated}}</span>
                </li>
                <li class="list-group-item">
                  <b>Payment for</b> <span class="pull-right">Marker</span>
                </li>
                <li class="list-group-item">
                  <b>Payment Status</b> <span class="pull-right">{{datas.ordersdetails.paymentStatus}}</span>
                </li>
                <li class="list-group-item">
                  <b>Amount Paid</b> <span class="pull-right"><i class="fa fa-dollar"></i> <b>{{datas.ordersdetails.Amount}}</b></span>
                </li>
                <li class="list-group-item">
                  <b>Date and Time of payment</b> <span class="pull-right">08-04-2016 / 20:35 </span>
                </li>
                <li class="list-group-item">
                  <b>Address of Owner</b> <span class="pull-right">290 Broadway, New York, NY 10007, USA</span>
                </li>
              </ul>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
        <div class="col-md-6">
          <!-- Profile Image -->
          <div class="box box-success">
            <div class="box-body Verification_details">
              <p class="lead margin-top-30"><strong>Atachments</strong></p>
              <div class="sticker">
                <img alt="140x140" class="img-thumbnail" src="admin/img/thumb.png" data-holder-rendered="true">
                <img alt="140x140" class="img-thumbnail" src="admin/img/thumb.png" data-holder-rendered="true">
                <img alt="140x140" class="img-thumbnail" src="admin/img/thumb.png" data-holder-rendered="true">
                <img alt="140x140" class="img-thumbnail" src="admin/img/thumb.png" data-holder-rendered="true">
              </div>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
      <div class="row">
        <div class="col-md-12">
          <a href="/admin/verifyList" class="btn btn-lg btn-warning">Back</a>
      </div>
      </div>

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <% include ./partials/footer %>
</div>
<!-- ./wrapper --> 

<!-- jQuery 2.2.3 --> 
<script src="admin/plugins/jQuery/jquery-2.2.3.min.js"></script> 
<!-- Bootstrap 3.3.6 --> 
<script src="admin/js/bootstrap.min.js"></script> 
<!-- DataTables --> 
<script src="admin/plugins/datatables/jquery.dataTables.min.js"></script> 
<script src="admin/plugins/datatables/dataTables.bootstrap.min.js"></script> 
<script src="admin/js/app.min.js"></script>

<script>
  $(function () {
    $('#user_list').DataTable({
      "paging": true,
      "lengthChange": true,
      "searching": true,
      "ordering": true,
      "info": true,
      "autoWidth": false
    });
  });
</script>
</body>
</html>

<!DOCTYPE html>
<html ng-app="app" ng-controller="user_Ctrl" ng-init="getuserDetails();">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Hasty Parking | Data Tables</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="admin/css/bootstrap.min.css">
  <link rel="stylesheet" href="admin/css/styles.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -- >
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="admin/plugins/datatables/dataTables.bootstrap.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="admin/css/admin.min.css">
  <!-- Admin Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
        <!--  <script src="jquery-3.1.1.min.js"></script> -->

  <link rel="stylesheet" href="admin/css/all-skins.min.css">
  <script type="text/javascript" src="admin/js/angularjs/default/angular.min.js"></script>
  <script type="text/javascript" src="admin/js/angularjs/user/app.js"></script>
  <script type="text/javascript" src="admin/js/angularjs/user/designer/userlist.js"></script>
  
</head>
<body class="hold-transition skin-green sidebar-mini">
<div class="wrapper">
  <% include ./partials/header %>
  <!-- Left side column. contains the logo and sidebar -->
 <% include ./partials/side-bar %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper"> 
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1> Users List </h1>
      <ol class="breadcrumb">
        <li><i class="fa fa-dashboard"></i> Home</li>
        <li class="active">Users List</li>
      </ol>
    </section>
    
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <!-- /.box-header -->
            <div class="box-body">
              <table id="user_list" class="table table-bordered table-striped">
                <thead>
                  <tr>
                    <th>S.No </th>
                    <th>Customer Name</th>
                    <th>Email Address</th>
                    <th>Contact Number</th>
                    <th>Active From</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <tr ng-cloak class='ng-cloak' ng-repeat="data in datas">
                    <td>{{data.s_no}}</td>
                    <td>{{data.customername}}</td>
                    <td>{{data.email}}</td>
                    <td>{{data.contactnumber}}</td>
                    <td>{{data.activefrom | date:'dd-MM-yyyy'}}</td>
                    <td><label class="switch margin-bottom-none">
                      <input type="checkbox" name="disable">
                      <div class="slider round"></div>
                      </label>
                      <a href="" ng-click="userdetail(data.userid)" userid="{{data.userid}}" ng-model="data.userid" class="u_view"><i  class="fa fa-eye pull-right fa-2x" aria-hidden="true"></i></a>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
            <!-- /.box-body --> 
          </div>
          <!-- /.box --> 
        </div>
        <!-- /.col --> 
      </div>
      <!-- /.row --> 
    </section>
    <!-- /.content --> 
  </div>
  <!-- /.content-wrapper -->
  <% include ./partials/footer %>
</div>
<!-- ./wrapper --> 

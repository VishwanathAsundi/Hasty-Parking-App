<!doctype html>
<html lang="en" ng-app="app" ng-controller="receivedbooking" ng-init="getpastbooking();">

<head>
<meta charset="utf-8">
<title>Hasty Parking | Details page</title>
<!-- Bootstrap -->
<link href="website/css/bootstrap.min.css" rel="stylesheet">
<link href="website/css/styles.css" rel="stylesheet">
<link href="website/css/responsive.css" rel="stylesheet">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
<script type="text/javascript" src="website/js/angularjs/default/angular.min.js"></script>
<script type="text/javascript" src="website/js/angularjs/webjs/app.js"></script>
<script type="text/javascript" src="website/js/angularjs/webjs/designer/receivedbooking.js"></script>
<script type="text/javascript" src="website/js/angularjs/webjs/designer/dropdown.js"></script>

</head>

<body>
<header>
  <div class="container-fluid">
    <div class="col-md-12">
      <nav class="navbar navbar-default">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false"> <span class="sr-only">Mobile Menu</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
          <a class="navbar-brand logo text-uppercase" href="/website/index"><b>Hasty</b>Parking</a> </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
     
          <ul class="nav navbar-nav navbar-right" ng-controller="dropdown_ctrl">  
            <li><a href="/website/index">Book Parking</a></li>
            <li><a href="/website/rentspace">Rent Your Space</a></li>
            <li><a href="/website/freeparking">Free Parking</a></li>
            <li class="dropdown"> <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <i class="fa fa-user" style="margin-right:5px;"></i> {{datas.f_name}}<span class="glyphicon glyphicon-chevron-down"></span> </a>
              <ul class="dropdown-menu">
                <li>
                  <div class="navbar-login">
                    <div class="row">
                      <div class="col-lg-4">
                        <p class="text-center"> <span class="glyphicon glyphicon-user icon-size"></span> </p>
                      </div>
                      <div class="col-lg-8">
                        <p class="text-left"><strong>{{datas.f_name}} {{datas.l_name}}</strong></p>
                        <p class="text-left small">{{datas.email}}</p>
                        <p class="text-left"> <a href="/website/profile" class="btn btn-primary btn-block btn-sm">Profile</a> </p>
                      </div>
                    </div>
                  </div>
                </li>
                <li class="divider navbar-login-session-bg"></li>
                <li><a href="#">Account Settings <span class="glyphicon glyphicon-cog pull-right"></span></a></li>
                <li class="divider"></li>
                <li><a href="#">Bookings <span class="glyphicon glyphicon-stats pull-right"></span></a></li>
                <li class="divider"></li>
                <!-- CHECK COUNT!!//ATISHAY NOTE -->
                <li><a href="#">Parking Spaces<span class="badge pull-right">{{datas.count}}</span></a></li>
                <li class="divider"></li>
                <li><a href="#">Transfers <span class="glyphicon glyphicon-heart pull-right"></span></a></li>
                <li class="divider"></li>
                <li><a href="#" ng-click="signout();">Sign Out <span class="glyphicon glyphicon-log-out pull-right"></span></a></li>
              </ul>
            </li>
          </ul>
        </div>
        <!-- /.navbar-collapse --> 
      </nav>
    </div>
  </div>
</header>
<ul class="sub-menu">
  <li><a href="/website/rentspace">Add Spaces</a></li>
  <li><a href="/website/myspacelist">My Spaces</a></li>
  <li class="active"><a href="/website/receivedbookings">Received Booking</a></li>
  <li><a href="/website/mybooking">My Bookings</a></li>
</ul>
<div class="container">
   <form id="contact" action="" method="post">
    <div class="card clearfix receivedbooking">
      <div class="tab-pane" style="margin-top: 30px;">
        <ul class="nav nav-tabs">
          <li class="active"><h3><a data-toggle="tab" href="#pastBooking">Past Booking</a></h3></li>
          <li><h3><a data-toggle="tab" href="#currentBooking">Current Booking</a></h3></li>
        </ul>
        <div class="tab-content">
          <div id="pastBooking" class="tab-pane fade in active">
            <table class="table table-responsive">
           <thead>
                <tr>
                  <th>S.No</th>
                  <th>Space Location</th>
                  <th>Parking Start Date</th>
                  <th>Parking End Date</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                 <tr ng-repeat="data in bookedhistory">
                    <th scope="row">{{$index+1}}</th>
                    <td>{{data.mySpaceName}} {{data.parkingPlaceName}}</td>
                    <td>{{data.start}}</td>
                    <td>{{data.end}}</td>
                    <td>
                      <a href="/website/receivedbookingdetails" ng-click ="getparkingid(data,data.userdetails);" class="btn btn-green">View</a> 
                      
                    </td>
                  </tr>
                         
              
              
            
              </tbody>
            </table>
          </div>
          <div id="currentBooking" class="tab-pane fade in">
              <table class="table table-responsive">
                <thead>
                  <tr>
                    <th>S.No</th>
                    <th>Space Location</th>
                    <th>Parking Start Date</th>
                    <th>Parking End Date</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                <tr ng-repeat="data1 in upcoming">
                    <th scope="row">{{$index+1}}</th>
                    <td>{{data1.mySpaceName}} {{data1.parkingPlaceName}}</td>
                    <td>{{data1.start}}</td>
                    <td>{{data1.end}}</td>
                    <td>
                      <a href="/website/receivedbookingdetails"   ng-click ="getparkingid(data1,data1.userdetails);" class="btn btn-green">View</a> 
                   
                    </td>
                  </tr>

                </tbody>
              </table>
            </div>
        </div>
      </div>
    </div>
  </form>
</div>
<footer>
  <div class="container-fluid">
    <div class="col-md-6 col-sm-6 col-xs-12">
      <p class="text-left">Copyright &copy; 2017 Hasty Parking</p>
    </div>
    <div class="col-md-6 col-sm-6 col-xs-12">
      <ul class="text-right social">
        <li><a href="#"> <span class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i class="fa fa-facebook fa-stack-1x"></i> </span></a></li>
        <li><a href="#"> <span class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i class="fa fa-twitter fa-stack-1x"></i> </span></a></li>
        <li><a href="#"> <span class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i class="fa fa-instagram fa-stack-1x"></i> </span> </a></li>
        <li><a href="#"> <span class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i class="fa fa-google-plus fa-stack-1x"></i> </span> </a></li>
      </ul>
    </div>
  </div>
</footer>
<!-- Bootstrap --> 
<script src="website/js/jquery-2.1.1.min.js"></script> 
<script src="website/js/bootstrap.min.js"></script> 
</body>
</html>
